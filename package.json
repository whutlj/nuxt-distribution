{
  "name": "nuxt-distribution",
  "version": "1.0.0",
  "description": "nuxt分销手机端",
  "author": "李杰",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build && nuxt start",
    "server": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "start": "nuxt build -a"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "axios": "^0.18.0",
    "blueimp-md5": "^2.10.0",
    "cross-env": "^5.2.0",
    "element-ui": "^2.4.6",
    "express": "^4.16.3",
    "import": "^0.0.6",
    "moment": "^2.22.2",
    "nuxt": "^2.0.0",
    "qs": "^6.5.2"
  },
  "config": {
    "nuxt": {
      "host": "127.0.0.1",
      "port": "3000"
    }
  },
  "devDependencies": {
    "@nuxtjs/proxy": "^1.2.4",
    "babel-eslint": "^8.2.1",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "mockjs": "^1.0.1-beta3",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "postcss-aspect-ratio-mini": "^0.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-px-to-viewport": "^0.0.3",
    "postcss-url": "^7.2.1",
    "postcss-viewport-units": "^0.1.4",
    "postcss-write-svg": "^3.0.1",
    "precss": "^3.1.2"
  }
}
